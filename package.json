	"signalk-webapp"
  ],
  "author": "Dirk Behrendt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/formifan2002/signalk-vaarweginformatie-blocked"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@signalk/server-admin-ui-dependencies": "^1.0.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
